{
    "name": "senml_webserver",
    "version": "1.0.0",
    "description": "This is the nodeJs project that serves as the frontend of the model",
    "scripts": {
        "test": "echo \"Error: no test specified\" && exit 1",
        "build": "tsc && cp -r Server/src/public/ dist/ && cp -r Server/src/views/ dist/ && npm run tailwind",
        "build-win": "tsc && copy Server\\src\\views\\* dist\\views && copy Server\\src\\public\\* dist\\public && npm run tailwind",
        "tailwind": "npx tailwindcss -i ./Server/src/index.css -o ./dist/public/index.css",
        "clean": "rm -rf dist/",
        "dev": "npm run build && tsc-watch --onSuccess \"node ./dist/app.js\"",
        "dev-win": "npm run build-win && tsc-watch --onSuccess \"node ./dist/app.js\"",
        "start": "npm run build && node dist/app.js",
        "hellnopostinstall": "prisma generate && npm run build",
        "hellnopostinstall-win": "prisma generate && npm run build-win",
        "just-dev": "tsc-watch --onSuccess \"node ./dist/app.js\""
    },
    "keywords": [
        "sentiment",
        "nlp",
        "mlpack"
    ],
    "author": "John Kelechukwu Obi",
    "license": "ISC",
    "dependencies": {
        "axios": "^1.6.7",
        "cors": "^2.8.5",
        "dotenv": "^16.4.5",
        "ejs": "^3.1.9",
        "express": "^4.18.2",
        "uuid": "^9.0.1"
    },
    "devDependencies": {
        "@prisma/client": "^5.10.2",
        "@types/cors": "^2.8.17",
        "@types/express": "^4.17.21",
        "@types/node": "^20.11.19",
        "@types/uuid": "^9.0.8",
        "prisma": "^5.10.2",
        "tailwindcss": "^3.4.1",
        "tsc-watch": "^6.0.4",
        "typescript": "^5.3.3"
    }
  }
  