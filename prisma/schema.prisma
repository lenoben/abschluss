generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("MONGODB_URL")
}

model error_log {
  id          String   @id @default(auto()) @map("_id") @db.ObjectId
  error_date  DateTime @default(now())
  name        String?
  message     String?
  user_id     String?  @db.ObjectId
  site_user   site_user? @relation(fields: [user_id], references: [id])
}

model ffn {
  id           String  @id @default(auto()) @map("_id") @db.ObjectId
  text_id      String? @db.ObjectId
  probability  Float?
  result       Int?
  model_text   model_text? @relation(fields: [text_id], references: [id])
}

model nmc {
  id           String  @id @default(auto()) @map("_id") @db.ObjectId
  text_id      String? @db.ObjectId
  result       Int?
  model_text   model_text? @relation(fields: [text_id], references: [id])
}

model logreg {
  id           String  @id @default(auto()) @map("_id") @db.ObjectId
  text_id      String? @db.ObjectId
  probability  Float?
  result       Int?
  model_text   model_text? @relation(fields: [text_id], references: [id])
}

model model_text {
  id            String         @id @default(auto()) @map("_id") @db.ObjectId
  content       String?
  user_id       String?        @db.ObjectId
  ffn           ffn[]
  site_user     site_user?     @relation(fields: [user_id], references: [id])
  random_forest random_forest[]
  nmc           nmc[]
  logreg        logreg[]
}

model random_forest {
  id           String  @id @default(auto()) @map("_id") @db.ObjectId
  text_id      String? @db.ObjectId
  probability  Float?
  result       Int?
  model_text   model_text? @relation(fields: [text_id], references: [id])
}

model site_user {
  id          String       @id @default(auto()) @map("_id") @db.ObjectId
  name        String
  email       String       @unique
  error_log   error_log[]
  model_text  model_text[]
}